server {
    listen 80 default_server;
    listen [::]:80 default_server;
    root /var/www/readsb;
    index index.html;
    server_name _;

    # Path to application's JSON data
    location /data {
        alias /var/run/readsb/;
        access_log off;
    }

    location / {
        try_files $uri $uri/ =404;
    }
}

